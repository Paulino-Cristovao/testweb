# Contents of /embassy-mozambique-website/embassy-mozambique-website/.env.example

# Example environment variables for the Embassy of Mozambique website

# Django settings
DEBUG=True
SECRET_KEY='your-secret-key-here'
ALLOWED_HOSTS=localhost,127.0.0.1

# Database settings
DATABASE_NAME='your_database_name'
DATABASE_USER='your_database_user'
DATABASE_PASSWORD='your_database_password'
DATABASE_HOST='localhost'
DATABASE_PORT='5432'

# Email settings
EMAIL_BACKEND='django.core.mail.backends.smtp.EmailBackend'
EMAIL_HOST='smtp.your-email-provider.com'
EMAIL_PORT=587
EMAIL_USE_TLS=True
EMAIL_HOST_USER='your-email@example.com'
EMAIL_HOST_PASSWORD='your-email-password'

# Third-party API keys
OPENAI_API_KEY='your_openai_api_key_here'

# Other settings
LANGUAGE_CODE='en-us'
TIME_ZONE='UTC'
USE_I18N=True
USE_L10N=True
USE_TZ=True

# Add any other environment variables needed for your project here.